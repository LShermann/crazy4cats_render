<div id="<%= dom_id post %>">
<div class="card text-center">
  <div class="card-header">
    <%= post.title %>
  </div>
  <div class="card-body">
    <p class="card-text">
      <%= post.description %>
    </p>
  <a href="#" class="btn btn-primary">
    <%= link_to "Show this post", post %>
  </a>
</div>
  <div class="card-footer text-body-secondary">
    <%= post.updated_at %>
  </div>
</div>

